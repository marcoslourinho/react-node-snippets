	// create nodejs new model
	"nodejs new model": {
		"prefix": "nodem",
		"body": [
			"const mongoose = require('mongoose');",
			"",
			"const ${1}Schema = new mongoose.Schema({",
			"",
			"\tusername: {",
			"\t \ttype: String,",
			"\t \tunique: true,",
			"\t \trequired: true,",
			"\t \tlowercase: true,",
			"\t },",
			"",
			"\tcreatedAt: {",
			"\t \ttype: Date,",
			"\t \tdefault: Date.now,",
